<%=form_for @album, :html => { :multipart => true } do |f|%>
  <p>
    <%=f.label :title%>
    <%=f.text_field :title%>
  </p>
  <p>
    <%=f.label :description%>
    <%=f.text_field :description%>
      <div class="field">
   <% f.object.images.build if f.object.images.empty? %>
    <%= f.fields_for :images do |image| %>
     <%= render "image_fields", :f => image %>
    <% end %>
    <span class="add_another">
       <%= link_to_add_dynamic_form "Add Another", f, :images, 'image_fields' %>
    </span>
  </div>
  
  <%= f.submit %>
<%end%>